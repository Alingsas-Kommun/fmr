{
  "name": "alingsas-kommun/fmr",
  "type": "wordpress-theme",
  "version": "1.0.2",
  "license": "MIT",
  "description": "FÃ¶rtroendemannaregister is a wordpress theme for municipalities presenting their elected politicians and their assignments.",
  "homepage": "https://roots.io/sage/",
  "authors": [
    {
      "name": "Adam Alexandersson",
      "email": "adam.alexandersson@consid.com",
      "homepage": "https://github.com/considadam"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/roots/sage/issues",
    "forum": "https://discourse.roots.io/"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\": "database/"
    },
    "files": [
      "app/Core/Functions.php",
      "helpers.php"
    ]
  },
  "require": {
    "php": ">=8.3",
    "blade-ui-kit/blade-heroicons": "^2.6",
    "laravel/tinker": "^2.10",
    "livewire/livewire": "^3.6",
    "log1x/sage-directives": "^2.0",
    "phpoffice/phpspreadsheet": "^5.1",
    "pixelfear/composer-dist-plugin": "^0.1.6",
    "roots/acorn": "^5.0",
    "roots/acorn-prettify": "^1.0"
  },
  "require-dev": {
    "laravel/pint": "^1.20"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  },
  "extra": {
    "download-dist": [
      {
        "name": "public/build",
        "url": "https://github.com/Alingsas-Kommun/fmr/releases/download/{$version}/public-build-{$version}.zip",
        "path": "public/build"
      },
      {
        "name": "vendor",
        "url": "https://github.com/Alingsas-Kommun/fmr/releases/download/{$version}/vendor-{$version}.zip",
        "path": "vendor"
      }
    ]
  }
}
